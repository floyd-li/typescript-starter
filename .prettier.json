{
	"arrowParens": "always",
	"bracketSpacing": true,
	"endOfLine": "lf",
	"insertPragma": false,
	"printWidth": 80,
	"proseWrap": "preserve",
	"quoteProps": "always",
	"rangeEnd": null,
	"rangeStart": 0,
	"requirePragma": false,
	"semi": true,
	"singleQuote": true,
	"tabWidth": 2,
	"trailingComma": "all",
	"useTabs": false
}
